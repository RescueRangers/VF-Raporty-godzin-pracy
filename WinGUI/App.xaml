<Application x:Class="WinGUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WinGUI"
             xmlns:converter="clr-namespace:WinGUI.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <converter:NumerDoKolumnyConverter x:Key="KolumnyConverter"></converter:NumerDoKolumnyConverter>
            <Style x:Key="Wysrodkowanie">
                <Setter Property="Control.HorizontalAlignment" Value="Center"></Setter>
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>
